  <!-- main -->
  <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4">
    <h3>Hello <%=name%> </h3>

    <hr>
    <div class="row">
      <div class="col-sm-6">
        <div class="card">
          <div class="card-body">
            <p class="card-text">Set a weekly workout plan</p>
            <a href="/journal" class="btn btn-primary">Workout Journal</a>
          </div>
        </div>
      </div>
      <div class="col-sm-6">
        <div class="card">
          <div class="card-body">
            <p class="card-text">Log your food and track your calories</p>
            <a href="#" class="btn btn-primary">Calorie Tracker</a>
          </div>
        </div>
      </div>
    </div>

  </main>

  <!-- jQuery first, then Popper.js, then Bootstrap JS -->
  <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js"
